{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/data/(.*)",
      "destination": "/data/$1"
    },
    {
      "source": "/images/(.*)",
      "destination": "/images/$1"
    },
    {
      "source": "/player-images/(.*)",
      "destination": "/player-images/$1"
    },
    {
      "source": "/saturday-games/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/player/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/((?!api|_next|_vercel|favicon\\.ico|vercel\\.svg|vite\\.svg|data|images|player-images).*)",
      "destination": "/index.html"
    }
  ]
}
